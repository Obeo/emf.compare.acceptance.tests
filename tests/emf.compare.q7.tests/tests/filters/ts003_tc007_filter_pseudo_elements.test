------=_.description-216f885c-d591-38ce-8ea2-e4f8cb4d6ffa
Content-Type: text/plain
Entry-Name: .description

This test must validate that the filter "Pseudo confilcts elements" works.

TEST STEPS
	1) Select the 2 ecore models and run a 2-way comparison (select extlibraryLeft.ecore and extLibrary.ecore) 
	//asserts that the 2 models are selected
	//	verify that the extLibrary element in the tree contains 0 elements.
	2) Click on the menu Filters and unselect "Pseudo conflicts elements"
	//	verify that the extLibrary element in the tree contains 7 elements.
------=_.description-216f885c-d591-38ce-8ea2-e4f8cb4d6ffa--
------=_.content-0a7243a0-75d3-3d5f-9791-539de0e5b7ac
Content-Type: text/ecl
Entry-Name: .content

with [get-view "Package Explorer" | get-tree] {
    select "PseudoConflictsFilter/left.ecore" "PseudoConflictsFilter/origin.ecore" "PseudoConflictsFilter/right.ecore"
    get-menu "Compare With/Each Other" | click
    get-item "PseudoConflictsFilter/left.ecore" | get-property selected | equals true | verify-true
    get-item "PseudoConflictsFilter/origin.ecore" | get-property selected | equals true | verify-true
    get-item "PseudoConflictsFilter/right.ecore" | get-property selected | equals true | verify-true
}
with [get-window "Select Common Ancestor"] {
    get-button "\'/PseudoConflictsFilter/origin.ecore\'" | click
    get-button OK | click
}
get-tab-folder -index 1 | get-property activePage | contains "Compare" | verify-true

get-editor "Compare (\'PseudoConflictsFilter/right.ecore\' - \'PseudoConflictsFilter/left.ecore\' - \'PseudoConflictsFilter/right.ecore\')" 
    | get-tree | get-property itemCount | equals 0 | verify-true
get-editor "Compare (\'PseudoConflictsFilter/right.ecore\' - \'PseudoConflictsFilter/left.ecore\' - \'PseudoConflictsFilter/right.ecore\')" 
    | get-button Filters | click -arrow
get-menu "Pseudo conflicts differences" | click
get-editor "Compare (\'PseudoConflictsFilter/right.ecore\' - \'PseudoConflictsFilter/left.ecore\' - \'PseudoConflictsFilter/right.ecore\')" 
    | get-tree | select "> extlibrary" | expand-all
    
with [get-editor "Compare (\'PseudoConflictsFilter/right.ecore\' - \'PseudoConflictsFilter/left.ecore\' - \'PseudoConflictsFilter/right.ecore\')" 
    | get-tree] {
    get-item "> extlibrary" | get-property childCount | equals 10 | verify-true
    get-item "> extlibrary/> Book" | get-property index | equals 0 | verify-true
    get-item "> extlibrary/> Book" | get-property childCount | equals 2 | verify-true
    get-item "> extlibrary/> Library" | get-property index | equals 2 | verify-true
    get-item "> extlibrary/> Library" | get-property childCount | equals 1 | verify-true
    get-item "> extlibrary/> Library" | get-property "getItems().TreeItem[0].getItems().length" | equals 6 | verify-true
    get-item "> extlibrary/Periodical \\[eClassifiers move\\]" | get-property index | equals 4 | verify-true
    get-item "> extlibrary/Periodical \\[eClassifiers move\\]" | get-property childCount | equals 0 | verify-true
    get-item "> extlibrary" | get-item "Periodical \\[eClassifiers move\\]" -index 1 | get-property index | equals 6 
        | verify-true
    get-item "> extlibrary" | get-item "Periodical \\[eClassifiers move\\]" -index 1 | get-property childCount | equals 0 
        | verify-true
    get-item "> extlibrary/> VideoCassette" | get-property index | equals 3 | verify-true
    get-item "> extlibrary/> VideoCassette" | get-property childCount | equals 1 | verify-true
    get-item "> extlibrary/> VideoCassette" | get-property "getItems().TreeItem[0].getItems().length" | equals 6 
        | verify-true
    get-item "> extlibrary/Writer \\[eClassifiers add\\]" | get-property index | equals 5 | verify-true
    get-item "> extlibrary/Writer \\[eClassifiers add\\]" | get-property childCount | equals 0 | verify-true
    get-item "> extlibrary" | get-item "Writer \\[eClassifiers add\\]" -index 1 | get-property index | equals 7 
        | verify-true
    get-item "> extlibrary" | get-item "Writer \\[eClassifiers add\\]" -index 1 | get-property childCount | equals 0 
        | verify-true
}
