with [get-view "Package Explorer" | get-tree] {
    select "CompareDiagrams/ts010_tc014/ancestor.di" "CompareDiagrams/ts010_tc014/left.di" "CompareDiagrams/ts010_tc014/right.di"
    get-menu "Compare With/Each Other" | click
}
get-window "Select Common Ancestor" | get-button OK | click
get-editor "Compare (\'CompareDiagrams/ts010_tc014/right.di\' - \'CompareDiagrams/ts010_tc014/left.di\' - \'CompareDiagrams/ts010_tc014/right.di\')" 
    | get-tree 
    | select "> Diagram NewDiagram <Model> model/> Shape <Class> J/> BasicCompartment <Class> J/Shape <Property> Property1 \\[children add\\]" 
    | double-click
with [get-editor "Compare (\'CompareDiagrams/ts010_tc014/right.di\' - \'CompareDiagrams/ts010_tc014/left.di\' - \'CompareDiagrams/ts010_tc014/right.di\')" 
    | get-diagram -index 1 | get-edit-part -name model] {
    get-property "getViewport().getChildren().FreeformLayeredPane[0].getChildren().DiagramScalableFreeformLayeredPane[0].getChildren().Polyline[4].getPoints().getFirstPoint().x" 
        | equals 40 | verify-true
    get-property "getViewport().getChildren().FreeformLayeredPane[0].getChildren().DiagramScalableFreeformLayeredPane[0].getChildren().Polyline[4].getPoints().getFirstPoint().y" 
        | equals 429 | verify-true
    get-property "getViewport().getChildren().FreeformLayeredPane[0].getChildren().DiagramScalableFreeformLayeredPane[0].getChildren().Polyline[4].getPoints().getLastPoint().x" 
        | equals 130 | verify-true
    get-property "getViewport().getChildren().FreeformLayeredPane[0].getChildren().DiagramScalableFreeformLayeredPane[0].getChildren().Polyline[4].getPoints().getLastPoint().y" 
        | equals 429 | verify-true
}
