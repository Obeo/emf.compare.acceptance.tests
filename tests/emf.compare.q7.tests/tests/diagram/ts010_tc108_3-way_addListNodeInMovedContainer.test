with [get-view "Package Explorer" | get-tree] {
    select "CompareDiagrams/ts010_tc014/ancestor.di" "CompareDiagrams/ts010_tc014/left.di" "CompareDiagrams/ts010_tc014/right.di"
    get-menu "Compare With/Each Other" | click
}
get-window "Select Common Ancestor" | get-button OK | click
get-editor "Compare (\'CompareDiagrams/ts010_tc014/right.di\' - \'CompareDiagrams/ts010_tc014/left.di\' - \'CompareDiagrams/ts010_tc014/right.di\')" 
    | get-tree 
    | select "> Diagram NewDiagram <Model> model/> Shape <Class> J/> BasicCompartment <Class> J/Shape <Property> Property1 \\[children add\\]" 
    | double-click
with [get-editor "Compare (\'CompareDiagrams/ts010_tc014/right.di\' - \'CompareDiagrams/ts010_tc014/left.di\' - \'CompareDiagrams/ts010_tc014/right.di\')" 
    | get-diagram -index 1 | get-edit-part -name model] {
    
    lt [get-property "getViewport().getChildren().FreeformLayeredPane[0].getChildren().DiagramScalableFreeformLayeredPane[0].getChildren().Polyline[4].getPoints().getFirstPoint().x" -raw] 42 | assert-true "x != 40"
	gt [get-property "getViewport().getChildren().FreeformLayeredPane[0].getChildren().DiagramScalableFreeformLayeredPane[0].getChildren().Polyline[4].getPoints().getFirstPoint().x" -raw] 38 | assert-true "x != 40"
	lt [get-property "getViewport().getChildren().FreeformLayeredPane[0].getChildren().DiagramScalableFreeformLayeredPane[0].getChildren().Polyline[4].getPoints().getFirstPoint().y" -raw] 431 | assert-true "y != 429"
	gt [get-property "getViewport().getChildren().FreeformLayeredPane[0].getChildren().DiagramScalableFreeformLayeredPane[0].getChildren().Polyline[4].getPoints().getFirstPoint().y" -raw] 427 | assert-true "y != 429"			        
    
    lt [get-property "getViewport().getChildren().FreeformLayeredPane[0].getChildren().DiagramScalableFreeformLayeredPane[0].getChildren().Polyline[4].getPoints().getLastPoint().x" -raw] 132 | assert-true "x != 130"
	gt [get-property "getViewport().getChildren().FreeformLayeredPane[0].getChildren().DiagramScalableFreeformLayeredPane[0].getChildren().Polyline[4].getPoints().getLastPoint().x" -raw] 128 | assert-true "x != 130"
	lt [get-property "getViewport().getChildren().FreeformLayeredPane[0].getChildren().DiagramScalableFreeformLayeredPane[0].getChildren().Polyline[4].getPoints().getLastPoint().y" -raw] 431 | assert-true "y != 429"
	gt [get-property "getViewport().getChildren().FreeformLayeredPane[0].getChildren().DiagramScalableFreeformLayeredPane[0].getChildren().Polyline[4].getPoints().getLastPoint().y" -raw] 427 | assert-true "y != 429"
}
