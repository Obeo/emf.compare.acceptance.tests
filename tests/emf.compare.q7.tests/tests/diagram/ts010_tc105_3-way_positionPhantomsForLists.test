------=_.content-0a7243a0-75d3-3d5f-9791-539de0e5b7ac
Content-Type: text/ecl
Entry-Name: .content

with [get-view "Package Explorer" | get-tree] {
    select "CompareDiagrams/ts010_tc014/ancestor.di" "CompareDiagrams/ts010_tc014/left.di" "CompareDiagrams/ts010_tc014/right.di"
    get-menu "Compare With/Each Other" | click
}
get-window "Select Common Ancestor" | get-button OK | click
get-editor "Compare (\'CompareDiagrams/ts010_tc014/right.di\' - \'CompareDiagrams/ts010_tc014/left.di\' - \'CompareDiagrams/ts010_tc014/right.di\')" 
    | get-tree 
    | select "> Diagram NewDiagram <Model> model/> Shape <Class> H/> BasicCompartment <Class> H/Shape <Property> feature6 \\[children add\\]" 
    | double-click
with [get-editor "Compare (\'CompareDiagrams/ts010_tc014/right.di\' - \'CompareDiagrams/ts010_tc014/left.di\' - \'CompareDiagrams/ts010_tc014/right.di\')" 
    | get-diagram -index 1 | get-edit-part -name model] {
        
    lt [get-property "getViewport().getChildren().FreeformLayeredPane[0].getChildren().DiagramScalableFreeformLayeredPane[0].getChildren().RectangleFigure[4].getLocation().x" -raw] 331 | assert-true "x != 329"
	gt [get-property "getViewport().getChildren().FreeformLayeredPane[0].getChildren().DiagramScalableFreeformLayeredPane[0].getChildren().RectangleFigure[4].getLocation().x" -raw] 327 | assert-true "x != 329"
	lt [get-property "getViewport().getChildren().FreeformLayeredPane[0].getChildren().DiagramScalableFreeformLayeredPane[0].getChildren().RectangleFigure[4].getLocation().y" -raw] 352 | assert-true "y != 350"
	gt [get-property "getViewport().getChildren().FreeformLayeredPane[0].getChildren().DiagramScalableFreeformLayeredPane[0].getChildren().RectangleFigure[4].getLocation().y" -raw] 348 | assert-true "y != 350"			    
              
}
get-editor "Compare (\'CompareDiagrams/ts010_tc014/right.di\' - \'CompareDiagrams/ts010_tc014/left.di\' - \'CompareDiagrams/ts010_tc014/right.di\')" 
    | get-tree 
    | select "> Diagram NewDiagram <Model> model/> Shape <Class> H/> BasicCompartment <Class> H/Shape <Property> feature2 \\[children delete\\]" 
    | double-click
with [get-editor "Compare (\'CompareDiagrams/ts010_tc014/right.di\' - \'CompareDiagrams/ts010_tc014/left.di\' - \'CompareDiagrams/ts010_tc014/right.di\')" 
    | get-diagram -index 1 | get-edit-part -name model] {
        
    lt [get-property "getViewport().getChildren().FreeformLayeredPane[0].getChildren().DiagramScalableFreeformLayeredPane[0].getChildren().Polyline[4].getLocation().x" -raw] 331 | assert-true "x != 329"
	gt [get-property "getViewport().getChildren().FreeformLayeredPane[0].getChildren().DiagramScalableFreeformLayeredPane[0].getChildren().Polyline[4].getLocation().x" -raw] 327 | assert-true "x != 329"
	lt [get-property "getViewport().getChildren().FreeformLayeredPane[0].getChildren().DiagramScalableFreeformLayeredPane[0].getChildren().Polyline[4].getLocation().y" -raw] 339 | assert-true "y != 337"
	gt [get-property "getViewport().getChildren().FreeformLayeredPane[0].getChildren().DiagramScalableFreeformLayeredPane[0].getChildren().Polyline[4].getLocation().y" -raw] 335 | assert-true "y != 337"			    
}
with [get-editor "Compare (\'CompareDiagrams/ts010_tc014/right.di\' - \'CompareDiagrams/ts010_tc014/left.di\' - \'CompareDiagrams/ts010_tc014/right.di\')"] {
    get-button "Copy Current Change From Left To Right" | click
    get-tree 
        | select "> Diagram NewDiagram <Model> model/> Shape <Class> H/> BasicCompartment <Class> H/Shape <Property> feature6 \\[children add\\]" 
        | double-click
}
with [get-editor "Compare (\'CompareDiagrams/ts010_tc014/right.di\' - \'CompareDiagrams/ts010_tc014/left.di\' - \'CompareDiagrams/ts010_tc014/right.di\')" 
    | get-diagram -index 1 | get-edit-part -name model] {
        
    lt [get-property "getViewport().getChildren().FreeformLayeredPane[0].getChildren().DiagramScalableFreeformLayeredPane[0].getChildren().Polyline[4].getLocation().x" -raw] 331 | assert-true "x != 329"
	gt [get-property "getViewport().getChildren().FreeformLayeredPane[0].getChildren().DiagramScalableFreeformLayeredPane[0].getChildren().Polyline[4].getLocation().x" -raw] 327 | assert-true "x != 329"
	lt [get-property "getViewport().getChildren().FreeformLayeredPane[0].getChildren().DiagramScalableFreeformLayeredPane[0].getChildren().Polyline[4].getLocation().y" -raw] 365 | assert-true "y != 363"
	gt [get-property "getViewport().getChildren().FreeformLayeredPane[0].getChildren().DiagramScalableFreeformLayeredPane[0].getChildren().Polyline[4].getLocation().y" -raw] 361 | assert-true "y != 363"			    
}
