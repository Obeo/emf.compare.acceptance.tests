with [get-view "Package Explorer" | get-tree] {
    select "CompareDiagrams/ts010_tc002/ancestor.di" "CompareDiagrams/ts010_tc002/left.di" "CompareDiagrams/ts010_tc002/right.di"
    get-menu "Compare With/Each Other" | click
}
get-window "Select Common Ancestor" | get-button OK | click
with [get-editor "Compare (\'CompareDiagrams/ts010_tc002/right.di\' - \'CompareDiagrams/ts010_tc002/left.di\' - \'CompareDiagrams/ts010_tc002/right.di\')"] {
    get-tree 
        | select "> Diagram NewDiagram <Model> model/Shape <Class> D \\[children delete\\]/Shape <Class> E \\[children add\\]" 
        | double-click
    get-button "Show Ancestor Pane" | click
}
with [get-editor "Compare (\'CompareDiagrams/ts010_tc002/right.di\' - \'CompareDiagrams/ts010_tc002/left.di\' - \'CompareDiagrams/ts010_tc002/right.di\')"] {
    with [get-diagram -index 1 | get-edit-part -name model] {
        get-property "getViewport().getChildren().FreeformLayeredPane[0].getChildren().DiagramScalableFreeformLayeredPane[0].getChildren().RectangleFigure[4].getLineWidth()" 
            | equals 2 | verify-true
            
        lt [get-property "getViewport().getChildren().FreeformLayeredPane[0].getChildren().DiagramScalableFreeformLayeredPane[0].getChildren().RectangleFigure[4].getLocation().x" -raw] 125 | assert-true "x != 119"
		gt [get-property "getViewport().getChildren().FreeformLayeredPane[0].getChildren().DiagramScalableFreeformLayeredPane[0].getChildren().RectangleFigure[4].getLocation().x" -raw] 113 | assert-true "x != 119"
		lt [get-property "getViewport().getChildren().FreeformLayeredPane[0].getChildren().DiagramScalableFreeformLayeredPane[0].getChildren().RectangleFigure[4].getLocation().y" -raw] 156 | assert-true "y != 150"
		gt [get-property "getViewport().getChildren().FreeformLayeredPane[0].getChildren().DiagramScalableFreeformLayeredPane[0].getChildren().RectangleFigure[4].getLocation().y" -raw] 144 | assert-true "y != 150"			    
            
        lt [get-property "getViewport().getChildren().FreeformLayeredPane[0].getChildren().DiagramScalableFreeformLayeredPane[0].getChildren().RectangleFigure[4].getSize().width" -raw] 96 | assert-true "width != 90"
		gt [get-property "getViewport().getChildren().FreeformLayeredPane[0].getChildren().DiagramScalableFreeformLayeredPane[0].getChildren().RectangleFigure[4].getSize().width" -raw] 84 | assert-true "width != 90"
		lt [get-property "getViewport().getChildren().FreeformLayeredPane[0].getChildren().DiagramScalableFreeformLayeredPane[0].getChildren().RectangleFigure[4].getSize().height" -raw] 19 | assert-true "height != 13"
		gt [get-property "getViewport().getChildren().FreeformLayeredPane[0].getChildren().DiagramScalableFreeformLayeredPane[0].getChildren().RectangleFigure[4].getSize().height" -raw] 7 | assert-true "height != 13"			        
            
            
        get-property "getViewport().getChildren().FreeformLayeredPane[0].getChildren().DiagramScalableFreeformLayeredPane[0].getChildren().size()" 
            | equals 5 | verify-true
    }
    with [get-diagram -index 2 | get-edit-part -name model] {
        get-property "getViewport().getChildren().FreeformLayeredPane[0].getChildren().DiagramScalableFreeformLayeredPane[0].getChildren().Polyline[4].getLineWidth()" 
            | equals 2 | verify-true
            
        lt [get-property "getViewport().getChildren().FreeformLayeredPane[0].getChildren().DiagramScalableFreeformLayeredPane[0].getChildren().Polyline[4].getLocation().x" -raw] 124 | assert-true "x != 118"
		gt [get-property "getViewport().getChildren().FreeformLayeredPane[0].getChildren().DiagramScalableFreeformLayeredPane[0].getChildren().Polyline[4].getLocation().x" -raw] 112 | assert-true "x != 118"
		lt [get-property "getViewport().getChildren().FreeformLayeredPane[0].getChildren().DiagramScalableFreeformLayeredPane[0].getChildren().Polyline[4].getLocation().y" -raw] 155 | assert-true "y != 149"
		gt [get-property "getViewport().getChildren().FreeformLayeredPane[0].getChildren().DiagramScalableFreeformLayeredPane[0].getChildren().Polyline[4].getLocation().y" -raw] 143 | assert-true "y != 149"			       
        
        lt [get-property "getViewport().getChildren().FreeformLayeredPane[0].getChildren().DiagramScalableFreeformLayeredPane[0].getChildren().Polyline[4].getSize().width" -raw] 99 | assert-true "width != 93"
		gt [get-property "getViewport().getChildren().FreeformLayeredPane[0].getChildren().DiagramScalableFreeformLayeredPane[0].getChildren().Polyline[4].getSize().width" -raw] 87 | assert-true "width != 93"
		lt [get-property "getViewport().getChildren().FreeformLayeredPane[0].getChildren().DiagramScalableFreeformLayeredPane[0].getChildren().Polyline[4].getSize().height" -raw] 9 | assert-true "height != 3"
		gt [get-property "getViewport().getChildren().FreeformLayeredPane[0].getChildren().DiagramScalableFreeformLayeredPane[0].getChildren().Polyline[4].getSize().height" -raw] 0 | assert-true "height != 3"
        
        get-property "getViewport().getChildren().FreeformLayeredPane[0].getChildren().DiagramScalableFreeformLayeredPane[0].getChildren().RectangleFigure[5].getLineWidth()" 
            | equals 1 | verify-true
            
        lt [get-property "getViewport().getChildren().FreeformLayeredPane[0].getChildren().DiagramScalableFreeformLayeredPane[0].getChildren().RectangleFigure[5].getLocation().x" -raw] 120 | assert-true "x != 114"
		gt [get-property "getViewport().getChildren().FreeformLayeredPane[0].getChildren().DiagramScalableFreeformLayeredPane[0].getChildren().RectangleFigure[5].getLocation().x" -raw] 108 | assert-true "x != 114"
		lt [get-property "getViewport().getChildren().FreeformLayeredPane[0].getChildren().DiagramScalableFreeformLayeredPane[0].getChildren().RectangleFigure[5].getLocation().y" -raw] 81 | assert-true "y != 75"
		gt [get-property "getViewport().getChildren().FreeformLayeredPane[0].getChildren().DiagramScalableFreeformLayeredPane[0].getChildren().RectangleFigure[5].getLocation().y" -raw] 69 | assert-true "y != 75"			        
            
        lt [get-property "getViewport().getChildren().FreeformLayeredPane[0].getChildren().DiagramScalableFreeformLayeredPane[0].getChildren().RectangleFigure[5].getSize().width" -raw] 106 | assert-true "width != 100"
		gt [get-property "getViewport().getChildren().FreeformLayeredPane[0].getChildren().DiagramScalableFreeformLayeredPane[0].getChildren().RectangleFigure[5].getSize().width" -raw] 94 | assert-true "width != 100"
		lt [get-property "getViewport().getChildren().FreeformLayeredPane[0].getChildren().DiagramScalableFreeformLayeredPane[0].getChildren().RectangleFigure[5].getSize().height" -raw] 106 | assert-true "height != 100"
		gt [get-property "getViewport().getChildren().FreeformLayeredPane[0].getChildren().DiagramScalableFreeformLayeredPane[0].getChildren().RectangleFigure[5].getSize().height" -raw] 94 | assert-true "height != 100"    
            
        get-property "getViewport().getChildren().FreeformLayeredPane[0].getChildren().DiagramScalableFreeformLayeredPane[0].getChildren().size()" 
            | equals 6 | verify-true
    }
    get-diagram | get-edit-part -name model 
        | get-property "getViewport().getChildren().FreeformLayeredPane[0].getChildren().DiagramScalableFreeformLayeredPane[0].getChildren().size()" 
        | equals 4 | verify-true
}
